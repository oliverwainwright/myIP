{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListObject"
            	],
            "Resource": [ 
		"arn:aws:s3:::YOUR-BUCKET"
		]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            	],
            "Resource": [ 
		"arn:aws:s3:::YOUR-BUCKET/*"
		]
        }
    ]
}
